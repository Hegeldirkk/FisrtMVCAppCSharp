@model List<FisrtMVCAppCSharp.Models.HotelBookingModel>;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Hotel Booking</h1>
<hr />
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">GuestName</th>
            <th scope="col">DateStart</th>
            <th scope="col">DateEnd</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var hot in Model)
        {
            <tr>
                <th scope="row"> @hot.id</th>
                <td>@hot.GuestName</td>
                <td>@hot.DateStart</td>
                <td>@hot.DateEnd</td>
            </tr>
        }

    </tbody>
</table>
